{
    "kind": "collectionType",
    "collectionName": "shapes",
    "info": {
        "singularName": "shape",
        "pluralName": "shapes",
        "displayName": "Shape",
        "description": "Eyewear shapes for product categorization"
    },
    "options": {
        "draftAndPublish": false
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
            "type": "string",
            "required": true
        },
        "image": {
            "type": "media",
            "multiple": false,
            "required": true,
            "allowedTypes": [
                "images"
            ]
        },
        "count": {
            "type": "integer",
            "default": 0,
            "min": 0
        },
        "products": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::product.product"
        }
    }
}